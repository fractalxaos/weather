#!/bin/bash
# Prints size and last modification date of the error log generated by
# the UpdateRRD.py script.  Uses the ls and awk system commands.

DATA_FILE_PATH='/tmp/weather/weatherInputData.js'

while [ 1 ]; do
    sum1="$(md5sum "$DATA_FILE_PATH")"
    sleep 1
    sum2="$(md5sum "$DATA_FILE_PATH")"
    if ! [ "$sum1" = "$sum2" ];
    then
        cat $DATA_FILE_PATH

#    then
#        echo "Identical" > /dev/null
#    else
#        cat $DATA_FILE_PATH
    fi
done
