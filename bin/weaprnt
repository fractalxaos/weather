#!/bin/bash
# Prints out the weatherInputData file, created by the submit.php
# scripts, every time the file gets changed or updated.

DATA_FILE_PATH='/tmp/weather/weatherInputData.js'

while [ 1 ]; do
    sum1="$(md5sum "$DATA_FILE_PATH")"
    sleep 1
    sum2="$(md5sum "$DATA_FILE_PATH")"
    if ! [ "$sum1" = "$sum2" ];
    then
        WDATA=`cat $DATA_FILE_PATH`
        echo $WDATA
#    then
#        echo "Identical" > /dev/null
#    else
#        cat $DATA_FILE_PATH
    fi
done
